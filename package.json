  "name": "kicl",
  "version": "0.0.1",
  "description": "https://ki-cl/com",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "guide": "npx babel-node styleguidist server",
    "lint:stylelint": "stylelint 'project/src/**/*.{css,scss}' --fix --allow-empty-input",
    "lint:eslint": "eslint project/src/** --ext .jsx,.js,.tsx,.ts --fix --no-error-on-unmatched-pattern",
    "test:jest": "NODE_ENV=test jest --config ./.Jest/config.js --detectOpenHandles --changedSince=master --passWithNoTests",
    "test:cover": "npm run test -- --coverage --detectOpenHandles --updateSnapshot",
    "test:watch": "npm run test -- --watch",
    "dev": "DEBUG=true NODE_ENV=development NODE_OPTIONS=\"--max-old-space-size=8192\" webpack-dev-server --config .Webpack/development.babel.js",
    "prod": "DEBUG=true NODE_ENV=production babel-node .Webpack/production.js",
    "build": "DEBUG=true npm run prod -- --env.noBrowser"
  },
  "lint-staged": {
    "*.{css,js,jsx,ts,tsx,scss}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "npm run lint:stylelint"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:eslint",
      "npm run test:jest "
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenilam/Ki.CL"
  },

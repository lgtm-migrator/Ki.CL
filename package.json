  "scripts": {
    "guide": "npx babel-node styleguidist server",
    "test": "NODE_ENV=test jest --config ./.Jest/config.js --detectOpenHandles --changedSince=master",
    "test:watch": "npm run test -- --watch",
    "test:cover": "npm run test -- --coverage --detectOpenHandles --updateSnapshot",
    "lint": "npm run lint:tslint && npm run lint:stylelint",
    "lint:stylelint": "stylelint 'project/**/*.scss' --fix",
    "lint:tslint": "tslint '(project,.Webpack)/**/*.(js,jsx,ts,tsx)' --fix",
    "dev": "NODE_ENV=development webpack-dev-server --config .Webpack/development.babel.js",
    "prod": "NODE_ENV=production babel-node .Webpack/production.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "options": [
      "concurrent"
    ],
    "{.Webpack,project}/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:tslint",
      "git add"
    ],
    "project/**/*.scss": [
      "npm run lint:stylelint",
      "git add"
    ]
  },
